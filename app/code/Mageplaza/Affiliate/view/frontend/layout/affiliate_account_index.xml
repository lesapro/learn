<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="affiliate_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\Affiliate\Block\Account\Home" name="affiliate.account.home" template="Mageplaza_Affiliate::account/home.phtml" cacheable="false">
                <container name="credit.transaction">
                    <block class="Mageplaza\Affiliate\Block\Account\Home\Transaction" name="credit_transaction" template="Mageplaza_Affiliate::account/home/transactions.phtml"/>
                </container>
            </block>
        </referenceContainer>

    </body>
</page>
